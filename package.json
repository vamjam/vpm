{
  "name": "vpm",
  "version": "1.0.0",
  "main": "dist/main.cjs",
  "preload": "dist/preload.cjs",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "18.13.0"
  },
  "scripts": {
    "start": "yarn electron .",
    "dx": "conc --kill-others-on-fail -n main,renderer \"yarn dx:main\" \"yarn dx:renderer\"",
    "dx:main": "yarn build:main -w",
    "dx:renderer": "yarn vite --config src/renderer/vite.config.ts",
    "build:main": "node -r dotenv/config --loader ts-node/esm --experimental-json-modules scripts/build.ts",
    "build:renderer": "yarn vite --config src/renderer/vite.config.ts build",
    "prep": "yarn json2ts ./src/main/config/schema.json ./src/main/config/schema.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@fluentui/react-icons": "^2.0.191",
    "@sindresorhus/slugify": "1.1.2",
    "adm-zip": "^0.5.10",
    "allotment": "^1.17.1",
    "capital-case": "^1.0.4",
    "chalk": "4.1.2",
    "color": "^4.2.3",
    "dotenv": "^16.0.3",
    "electron": "^22.0.2",
    "electron-store": "^8.1.0",
    "got": "12.5.3",
    "hashids": "^2.2.10",
    "knex": "^2.4.1",
    "pretty-error": "^4.0.0",
    "pretty-ms": "7.0.1",
    "ps-list": "7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.6.2",
    "sharp": "^0.31.3",
    "sqlite3": "^5.1.4",
    "string-similarity": "^4.0.4",
    "styled-components": "^5.3.6",
    "styled-reset": "^4.4.5",
    "styled-system": "^5.1.5",
    "swr": "^2.0.0",
    "unique-slug": "^4.0.0",
    "usehooks-ts": "^2.9.1",
    "winston": "^3.8.2",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-for-react": "^1.0.4"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/adm-zip": "^0.5.0",
    "@types/color": "^3.0.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.10",
    "@types/sharp": "^0.31.1",
    "@types/string-similarity": "^4.0.0",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "csstype": "^3.0.11",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "esbuild": "^0.17.11",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "json-schema-to-typescript": "^12.0.0",
    "prettier": "^2.8.2",
