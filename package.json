{
  "name": "vpm",
  "version": "1.0.0",
  "main": "dist/main.cjs",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "dx": "yarn clean && node -r dotenv/config --loader ts-node/esm --experimental-json-modules scripts/watch.ts",
    "build": "yarn clean && node --loader ts-node/esm --experimental-json-modules scripts/build.ts",
    "clean": "rimraf dist",
    "db:gen": "yarn prisma generate --schema=./src/main/data/schema.prisma",
    "db:push": "yarn prisma db push --schema=./src/main/data/schema.prisma",
    "db:clean": "rimraf ./src/main/data/vpm.db",
    "images:clean": "rimraf images*/"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "@react-spring/web": "^9.5.5",
