    "adm-zip": "^0.5.9",
    "allotment": "^1.17.0",
    "capital-case": "^1.0.4",
    "chalk": "4.1.2",
    "color": "^4.2.3",
    "csstype": "^3.0.11",
    "dotenv": "^16.0.3",
    "electron": "^18.0.3",
    "electron-store": "^8.1.0",
    "got": "11.8.6",
    "pretty-error": "^4.0.0",
    "pretty-ms": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.4",
    "sharp": "^0.30.4",
    "string-similarity": "^4.0.4",
    "styled-components": "^5.3.6",
    "styled-reset": "^4.4.4",
    "styled-system": "^5.1.5",
    "swr": "^1.3.0",
    "winston": "^3.8.2",
    "xterm": "^5.0.0",
    "xterm-addon-fit": "^0.6.0",
    "xterm-for-react": "^1.0.4",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/adm-zip": "^0.5.0",
    "@types/color": "^3.0.3",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.6",
